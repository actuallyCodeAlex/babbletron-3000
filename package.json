  "bugs": {
    "url": "https://github.com/actuallyCodeAlex/babbletron-3000/issues"
  },
  "homepage": "https://github.com/actuallyCodeAlex/babbletron-3000#readme",
  "devDependencies": {
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "ava": "5.2.0",
    "copyfiles": "2.4.1",
    "typescript": "5.0.4",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "electron": "24.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "^5.83.1"
  }
