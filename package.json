{
  "name": "babbletron-3000",
  "version": "1.0.0",
  "description": "1 tool for creating and managing the babble of work.",
  "private": "true",
  "main": "./dist/main.js",
  "scripts": {
    "build": "npm run clean && npm run build:main && npm run build:renderer && npm run copy-files",
    "build:main": "tsc",
    "build:renderer": "webpack",
    "clean": "rimraf ./dist",
    "copy-files": "copyfiles -u 1 public/**/* dist/",
    "start": "electron .",
    "test": "ava"
  },
